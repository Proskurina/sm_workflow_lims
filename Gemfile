# A sample Gemfile
source "https://rubygems.org"

gem "puma"
gem "sinatra"
gem "sinatra-activerecord"
gem "activerecord"
gem "activesupport"
gem "mysql2"
gem "rake"
gem "pry"
gem "pry-nav"

# Asset Pipeline
gem "bootstrap-sass"
gem "sinatra-assetpack"

group :development do
  gem 'rack-mini-profiler'
end

group :test do
  gem "rspec"
  gem "timecop"
  gem "database_cleaner"
  gem 'factory_girl'
  gem "capybara"
  gem "poltergeist"
  gem 'launchy'
end
